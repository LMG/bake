Adapt {
    ExecutableConfig __ALL__, project: __ALL__, type: extend {
      PostSteps {
        CommandLine "echo adapt_$(ProjectName)_$(ConfigName)_$(LIST_ADAPT)_adapt"
      }
  }
}

