  LibraryConfig test1 {
    Files "src/*1a.cpp", compileOnly: true
    Files "src/lib1b.cpp"
  }

  LibraryConfig test2 {
    Files "src/lib2.cpp", compileOnly: true
  }

  ExecutableConfig test {
