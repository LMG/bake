Project {

  LibraryConfig test_lib {
    Files "src/lib.cpp"
    Toolchain {
      Compiler CPP, prefix: "echo CPPPREFIX"
    }
  }

  ExecutableConfig test_main {
    Files "src/main.cpp"
    Dependency config: test_lib
    DefaultToolchain GCC
  }

  LibraryConfig test_lib_prio {
    Files "src/lib.cpp"
  }

  ExecutableConfig test_main_prio1 {
    Files "src/main.cpp"
    Files "src/c.c"
    Files "src/asm.s"
    Dependency config: test_lib_prio
    
    Set CPPCompilerPrefix, value: "CPPCOMPILERPREFIX2"
    Set CCompilerPrefix, value: "CCOMPILERPREFIX2"
    Set ASMCompilerPrefix, value: "ASMCOMPILERPREFIX2"
    Set ArchiverPrefix, value: "ARCHIVERPREFIX2"
    Set LinkerPrefix, value: "LINKERPREFIX2"
   
    Set CompilerPrefix, value: "COMPILERPREFIX3"

    DefaultToolchain GCC {
        Linker prefix: "echo LINKERPREFIX1"
        Archiver prefix: "echo ARCHIVERPREFIX1"
        Compiler CPP, prefix: "echo CPPCOMPILERPREFIX1"
        Compiler C,   prefix: "echo CCOMPILERPREFIX1"
        Compiler ASM, prefix: "echo ASMCOMPILERPREFIX1"
    }
  }

  ExecutableConfig test_main_prio1_env {
    Files "src/main.cpp"
    Files "src/c.c"
    Files "src/asm.s"
    Dependency config: test_lib_prio

    DefaultToolchain GCC {
        Linker prefix: "echo LINKERPREFIX1"
        Archiver prefix: "echo ARCHIVERPREFIX1"
        Compiler CPP, prefix: "echo CPPCOMPILERPREFIX1"
        Compiler C,   prefix: "echo CCOMPILERPREFIX1"
        Compiler ASM, prefix: "echo ASMCOMPILERPREFIX1"
    }
  }

  ExecutableConfig test_main_prio2 {
    Files "src/main.cpp"
    Files "src/c.c"
    Files "src/asm.s"
    Dependency config: test_lib_prio
    
    Set CPPCompilerPrefix, value: "CPPCOMPILERPREFIX2"
    Set CCompilerPrefix, value: "CCOMPILERPREFIX2"
    Set ASMCompilerPrefix, value: "ASMCOMPILERPREFIX2"
    Set ArchiverPrefix, value: "ARCHIVERPREFIX2"
    Set LinkerPrefix, value: "LINKERPREFIX2"
   
    Set CompilerPrefix, value: "COMPILERPREFIX3"

    DefaultToolchain GCC {
      Compiler ASM {
      }
    }
  }

  ExecutableConfig test_main_prio2_env {
    Files "src/main.cpp"
    Files "src/c.c"
    Files "src/asm.s"
    Dependency config: test_lib_prio
    
    DefaultToolchain GCC {
      Compiler ASM {
      }
    }
  }

  ExecutableConfig test_main_prio3 {
    Files "src/main.cpp"
    Files "src/c.c"
    Files "src/asm.s"
    Dependency config: test_lib_prio
   
    Set CompilerPrefix, value: "COMPILERPREFIX3"

    DefaultToolchain GCC
  }

  ExecutableConfig test_main_prio3_env {
    Files "src/main.cpp"
    Files "src/c.c"
    Files "src/asm.s"
    Dependency config: test_lib_prio
   
    DefaultToolchain GCC
  }


}
